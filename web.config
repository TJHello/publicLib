<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <directoryBrowse enabled="true" />
        <staticContent>
            <mimeMap fileExtension=".md5" mimeType="text/plain" />
            <mimeMap fileExtension=".sha1" mimeType="text/plain" />
            <mimeMap fileExtension=".aar" mimeType="application/x-zip-compressed" />
            <mimeMap fileExtension=".pom" mimeType="text/xml" />
        </staticContent>
        <security>
            <requestFiltering>
                <fileExtensions>
                    <add fileExtension=".apks" allowed="true" />
                </fileExtensions>
            </requestFiltering>
        </security>
    </system.webServer>
</configuration>
